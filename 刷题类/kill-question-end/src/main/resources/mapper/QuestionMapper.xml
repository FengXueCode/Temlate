<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zfizwy.killquestionend.mapper.QuestionMapper">

    <resultMap id="BaseResultMap" type="cn.zfizwy.killquestionend.entity.Question">
            <id property="questionId" column="question_id" jdbcType="VARCHAR"/>
            <result property="questionTitle" column="question_title" jdbcType="VARCHAR"/>
            <result property="options" column="options" jdbcType="OTHER"/>
            <result property="state" column="state" jdbcType="INTEGER"/>
            <result property="answer" column="answer" jdbcType="VARCHAR"/>
            <result property="analysis" column="analysis" jdbcType="VARCHAR"/>
            <result property="questionBankId" column="question_bank_id" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        question_id,question_title,options,
        state,answer,analysis,
        question_bank_id
    </sql>
</mapper>
